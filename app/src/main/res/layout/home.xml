<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView"
        android:layout_width="196dp"
        android:layout_height="47dp"
        android:fontFamily="sans-serif-light"
        android:text="NAVEGADOR"
        android:textColor="#060A50"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="104dp" />

    <Button
        android:id="@+id/btLogin"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:onClick="clicarLogin"
        android:text="@string/btLogin"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="220dp" />

    <Button
        android:id="@+id/btCadastro"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:onClick="clicarCadastro"
        android:text="@string/btCadastro"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="358dp" />

    <Button
        android:id="@+id/btSobre"
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:onClick="clicarSobre"
        android:text="@string/btSobre"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="499dp" />

</androidx.constraintlayout.widget.ConstraintLayout>